function speedfire()
  spd = gg.choice({
      '‚åú ·¥è…¥ ‚åü',
      '‚åú ·¥è“ì“ì ‚åü',
      ' ‚ü™  ô·¥Ä·¥Ñ·¥ã ‚ü´ ' }, nil,
    '‚ó§                                                       ‚ó•\n               ‚óÑ SpeedFire ‚ñ∫               \n‚ó£                                                       ‚ó¢')

  if spd == 1 then
    gg.toast('[' .. username .. ']' .. ' Loading...')
    gg.sleep(200)
    gg.setSpeed(1.2)
    gg.toast('[' .. username .. ']' .. ' Cheat Active ‚úÖ')
  end
  if spd == 2 then
    gg.toast('[' .. username .. ']' .. ' Loading...')
    gg.sleep(200)
    gg.setSpeed(1.0)
    gg.toast('[' .. username .. ']' .. ' Cheat Deactive ‚úÖ')
  end
  if spd == 3 then HOME() end
end


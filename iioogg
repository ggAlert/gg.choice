function silent()
  WH1 = gg.choice({
    "üî´ü¶∞SILENT„ÄêHEAD„Äë",
    "‚ñ∂Ô∏èüö∂üèæ‚Äç‚ôÇÔ∏èSILENT„ÄêBODY„Äë",
    "‚Å¶‚Å¶                            üîô„ÄéBACK„Äèüîô",
  }, nil, "üáÆüá∑[S]amp [C]heats\n üíØcreator : @No_Pulse\n üî∞Channel : @Cheaters_Samp")
  if WH1 == 1 then
    hahaha = gg.choice({
        '‚åú ·¥è…¥ ‚åü',
        '‚åú ·¥è“ì“ì ‚åü',
        ' ‚ü™  ô·¥Ä·¥Ñ·¥ã ‚ü´ ' }, nil,
      '‚ó§                                                       ‚ó•\n               ‚óÑ silent head ‚ñ∫               \n‚ó£                                                       ‚ó¢')

    if hahaha == 1 then silentHead() end
    if hahaha == 2 then silentHeadoff() end
    if hahaha == 3 then silent() end
  end
  if WH1 == 2 then
    hah = gg.choice({
        '‚åú ·¥è…¥ ‚åü',
        '‚åú ·¥è“ì“ì ‚åü',
        ' ‚ü™  ô·¥Ä·¥Ñ·¥ã ‚ü´ ' }, nil,
      '‚ó§                                                       ‚ó•\n               ‚óÑ silent body ‚ñ∫               \n‚ó£                                                       ‚ó¢')

    if hah == 1 then silentBody() end
    if hah == 2 then silentBodyoff() end
    if hah == 3 then silent() end
  end


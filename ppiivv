-- on head
function silentHead()
  gg.processResume()
  gg.searchNumber("1,042,536,202", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  gg.getResults(400, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.processResume()
  gg.editAll("1,070,300,300", gg.TYPE_DWORD)
  gg.toast('[' .. username .. ']' .. " Cheat Activeâœ…")
  gg.clearResults()
  silent()
end


-- on body
function silentBody()
